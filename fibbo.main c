// A program in C to  call a function to print Fibonacci series 
#include<stdio.h>

void fibbo( int n ){
    int i, fib, fval=1,sval=2;
    printf("%d  %d  ", fval, sval);
    for(i=3; i<=n; i++){
       fib= fval+ sval;
       printf("%d  ",fib);
    fval= sval;
     sval=fib;}
}

int main() {
    int range;
    printf("Enter range: \n");
    scanf("%d",& range);
    printf("Fibbonacci series: \n");
    fibbo( range);
    return 0;
}
